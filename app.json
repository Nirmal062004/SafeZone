{
  "expo": {
    "name": "SafeZone",
    "slug": "safezone",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "54.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.example.safezone",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "SafeZone needs access to your location to show nearby safe places like hospitals and police stations."
      }
    },
    "android": {
      "package": "com.example.safezone",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A free women's safety app that shows nearby hospitals, police stations, and government offices using OpenStreetMap.",
    "privacy": "public",
    "dependencies": {
      "expo-location": "^14.0.0",
      "react-native-webview": "^11.23.1"
    },
    "platforms": ["ios", "android", "web"]
  }
}
